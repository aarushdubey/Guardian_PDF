{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "chmod +x railway_build.sh && ./railway_build.sh"
    },
    "deploy": {
        "startCommand": "python rag_engine/app.py",
        "restartPolicyType": "ON_FAILURE",
        "restartPolicyMaxRetries": 10
    }
}